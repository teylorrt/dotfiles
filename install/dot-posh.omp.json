{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "pwd": "osc99",
  "console_title_template": "{{.PWD}}",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#87FF00",
          "style": "plain",
          "template": "{{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "in ",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "home_icon": "~",
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#61AFEF",
          "style": "plain",
          "template": "{{ if .Env.HAS_DOT_NET }}<#ffffff>via</> .NET v{{ .Env.DOT_NET_VERSON }} {{ end }}",
          "type": "text"
        },
        {
          "foreground": "#98C379",
          "style": "plain",
          "template": "{{ if .Env.HAS_NODE }}<#ffffff>via</> \ued0d {{ .Env.NODE_VERSION }} {{ end }}",
          "type": "text"
        },
        {
          "foreground": "#C678DD",
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "\ue725 ",
            "branch_identical_icon": "",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "no_commits_icon": "\uf0c3 ",
            "fetch_status": true,
            "merge_icon": "\ue727 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "style": "plain",
          "template": "{{ .HEAD }}{{ if .Staging.Changed }}<#C678DD> \uf044 {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#E5C07B> \uf044 {{ .Working.String }}</>{{ end }} ",
          "type": "git"
        },
        {
          "foreground": "#E06C75",
          "style": "plain",
          "template": "Î» ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
